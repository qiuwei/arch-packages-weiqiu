pkgname=vim-paredit
pkgver=0.9.9
_scriptid=3998
pkgrel=1
pkgdesc="Provide Emacs like Paredit Mode: Structured Editing of Lisp S-expressions "
arch=(i686 x86_64)
url="http://www.vim.org/scripts/script.php?script_id=3998"
license=('custom')
depends=(vim)
groups=('vim-plugins')
install=vimdoc.install
source=(paredit.vim::http://www.vim.org/scripts/download_script.php?src_id=${_scriptid}
        )
md5sums=('d8df3073939c447ca4a0029f70f6dc3a')

build() {
    install -Dm644 ${srcdir}/paredit.vim ${pkgdir}/usr/share/vim/vim73//plugin/paredit.vim || return 1
}

